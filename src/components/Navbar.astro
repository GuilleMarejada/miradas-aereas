---
import Image from "astro/components/Image.astro";
import Logo from "../assets/LogoMALimpio.webp";
import LogoInsta from "../assets/LogoInsta.webp";
import LogoYoutube from "../assets/LogoYotube.webp";
---

<nav
  class="flex justify-between bg-[#bebebe] shadow-sm rounded-b-xl p-4 place-items-center"
>
  <div class="">
    <Image src={Logo} alt={"Logo"} width={100} height={100} />
  </div>
  <div class="flex gap-4 h-full place-items-center">
    <a href="https://www.instagram.com/miradasaereasfilms/">
      <Image src={LogoInsta} alt={"Logo"} width={50} height={50} />
    </a>
    <a href="https://www.youtube.com/@Carloscv46"
      ><Image src={LogoYoutube} alt={"Logo"} width={70} height={50} /></a
    >
  </div>
</nav>
